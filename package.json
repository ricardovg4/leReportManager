{
  "name": "lereportmanager",
  "version": "1.0.0",
  "description": "LE report manager",
  "main": "index.js",
  "scripts": {
    "server": "nodemon ./server/server.js",
    "css-build": "node-sass --omit-source-map-url client/sass/mystyles.scss client/public/css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "dev": "npm-run-all --parallel css-watch server"
  },
  "keywords": [
    "LE",
    "lighting",
    "ever",
    "report",
    "manager"
  ],
  "author": "Ricardo Velasco",
  "license": "ISC",
  "dependencies": {
    "bulma": "^0.9.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.11"
  },
  "devDependencies": {
    "bcrypt": "^5.0.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  }
}
